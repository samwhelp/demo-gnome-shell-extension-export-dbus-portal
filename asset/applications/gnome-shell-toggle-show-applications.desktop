[Desktop Entry]
Version=1.1
Type=Application
Name=Toggle Show Applications
Comment=Open GNOME Shell applications list.
NoDisplay=false
Icon=show-grid
Exec=gdbus call --session --dest org.gnome.Shell --object-path /org/gnome/Shell/Extensions/ShellExportDbusPortal --method org.gnome.Shell.Extensions.ShellExportDbusPortal.ToggleShowApplications
